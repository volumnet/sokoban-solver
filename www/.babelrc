{
  presets: [
    [
      '@babel/preset-env',
      {
        "corejs": { "version":3 },
        "useBuiltIns": "entry",
        "loose": true,
        "forceAllTransforms": true
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"

  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      { runtime: "automatic" },
    ],
  ],
}